//Program to add Two numbers
class Main {
  public static void main(String[] args) {
    double r= 6;
    double area;
    final double PI=3.14;
    area= (3.14*r*r);
       System.out.println("Area of ciricle is "+ area);

    double r1=6.7;
    double area1= (PI*r1*r1);
      System.out.println("Area of ciricle is "+ area1);
  }
}
